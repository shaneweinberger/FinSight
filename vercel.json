{
    "rewrites": [
        {
            "destination": "/flask-server/api/index.py",
            "source": "/api/(.*)"
        },
        {
            "destination": "/client/build/static/$1",
            "source": "/static/(.*)"
        },
        {
            "destination": "/client/build/$1.$2",
            "source": "/(.*)\\.(.*)"
        },
        {
            "destination": "/client/build/index.html",
            "source": "/(.*)"
        }
    ]
}